<resources>

    <style name="start_up_style_item_bg">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingLeft">40dp</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingRight">40dp</item>
    </style>

    <style name="start_up_style_item_et">
        <item name="android:background">@drawable/common_bg_underline</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/start_up_color_login_text</item>
        <item name="android:textColorHint">@color/common_color_hint</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="start_up_style_item_btn_main">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/start_up_et_to_others</item>
        <item name="android:background">@color/common_main_color</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">17dp</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="start_up_style_item_btn_deputy">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:paddingLeft">30dp</item>
        <item name="android:paddingRight">30dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/common_color_hint</item>
    </style>

    <!--获取图形验证码按钮-->
    <style name="start_up_style_item_btn_verification">
        <item name="android:layout_width">90dp</item>
        <item name="android:layout_height">33dp</item>
        <item name="android:background">@drawable/common_bg_corner_main_16</item>
        <item name="android:textSize">13dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="CustomCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/common_check_box</item>
        <item name="android:clickable">false</item>
    </style>
</resources>
